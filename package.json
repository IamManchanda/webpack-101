{
  "name": "webpack-workshop-2018",
  "version": "1.0.0",
  "description": "Learning resources for the webpack academy workshop series for 2018",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.mode development --hot",
    "build": "rm -rf ./dist && webpack --env.mode production",
    "build:analyze": "yarn build --env.presets analyze",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "debug:dev": "yarn debug --env.mode development",
    "debug:prod": "yarn debug --env.mode production"
  },
